<body>
<div id='loDeDentro'>
<h1>Concert Platform</h1>
	<% @concerts.each do |concert|%>
	<div id='especial'><strong><%= concert.band_name %></strong>
		&nbsp&nbsp&nbsp&nbsp|&nbsp&nbsp&nbsp&nbsp
		<%= link_to 'Show',  concert_path(concert)%>
